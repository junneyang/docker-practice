					content    +=   	'<iframe id="metrics_data_view_' + node.id.toString() + '" src="" width="100%" height="500" frameborder="0" style=" "></iframe>';
					addTab(node.text, node.id, content);
					$("#metrics_data_view_" + node.id.toString()).attr('src', node.monitorDataLink);
				}
			});
		});
